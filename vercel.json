{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/social-app" } },
    { "src": "dist/social-app-server/server.mjs", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/social-app-server/server.mjs" }
  ],
  "functions": {
    "api/index.mjs": {
      "runtime": "nodejs18.x"
    }
  }
}
